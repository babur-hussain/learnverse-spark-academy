{
  "name": "learnverse-spark-academy",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "echo 'Please specify target: npm run dev:web or npm run dev:app' && exit 1",
    "dev:web": "VITE_TARGET=web VITE_HOST=0.0.0.0 vite",
    "dev:app": "VITE_TARGET=app VITE_HOST=0.0.0.0 vite",
    "dev:app:lan": "VITE_TARGET=app VITE_HOST=0.0.0.0 VITE_HMR_HOST=$LAN_HOST vite",
    "build": "VITE_TARGET=web tsc && VITE_TARGET=web vite build",
    "build:web": "VITE_TARGET=web tsc && VITE_TARGET=web vite build",
    "build:app": "VITE_TARGET=app tsc && VITE_TARGET=app vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "android:live": "VITE_TARGET=app VITE_HOST=0.0.0.0 VITE_DEV_URL=http://$LAN_HOST:8080 npx cap run android --external",
    "security:test": "node scripts/security-test.js",
    "security:audit": "npm audit && npm run security:test",
    "security:fix": "npm audit fix && npm run security:test",
    "setup:env": "./scripts/setup-env.sh",
    "build:android": "npm run build:app && npx cap sync android",
    "build:android-debug": "npm run build:android && cd android && ./gradlew assembleDebug",
    "build:android-release": "npm run build:android && cd android && ./gradlew assembleRelease",
    "build:android-bundle": "npm run build:android && cd android && ./gradlew bundleRelease",
    "build:ios": "npm run build:app && npx cap sync ios",
    "build:ios-debug": "npm run build:ios && cd ios && xcodebuild -workspace App.xcworkspace -scheme App -configuration Debug -destination generic/platform=iOS build",
    "build:ios-release": "npm run build:ios && cd ios && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release -destination generic/platform=iOS -archivePath App.xcarchive archive",
    "clean": "npx cap clean && rm -rf dist node_modules/.cache",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android"
  },
